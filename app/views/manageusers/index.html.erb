<div class="container navless-container">
  <table class="table table-striped">
    <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Created at</th>
    </tr>
    </thead>
    <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.created_at %></td>
    </tr>
  <% end %>
  </tbody>
  </table>
</div>